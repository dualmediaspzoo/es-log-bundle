{
    "name": "dualmedia/es-log-bundle",
    "description": "Bundle for tracking changes and history of objects",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "logs",
        "logging",
        "elasticsearch",
        "symfony",
        "doctrine",
        "orm"
    ],
    "require": {
        "php": "^8.3",
        "symfony/framework-bundle": "^6.4 || ^7.3",
        "symfony/cache": "^6.4 || ^7.3",
        "symfony/security-bundle": "^6.4 || ^7.3",
        "symfony/event-dispatcher": "^6.4 || ^7.3",
        "ruflin/elastica": "^7.1 || ^8",
        "doctrine/orm": "^2.15 || ^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "jetbrains/phpstorm-attributes": "*",
        "matthiasnoback/symfony-dependency-injection-test": "^6",
        "pedrotroller/php-cs-custom-fixer": "^2",
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-phpunit": "^2",
        "spaze/phpstan-disallowed-calls": "^4",
        "easycorp/easyadmin-bundle": "^4",
        "twig/twig": "^3",
        "phpunit/phpunit": "^12",
        "pkly/phpunit-service-create-trait": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DualMedia\\EsLogBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DualMedia\\EsLogBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "pkly",
            "homepage": "https://github.com/pkly",
            "role": "Author"
        },
        {
            "name": "Bartosz Urbaniak <barturb>",
            "homepage": "https://github.com/barturb",
            "role": "Bug fixes and general help"
        },
        {
            "name": "ozgaB",
            "homepage": "https://github.com/ozgaB",
            "role": "Tests and general fixes"
        }
    ],
    "minimum-stability": "stable",
    "conflict": {
        "easycorp/easyadmin-bundle": "<4.14 || ^5"
    }
}
